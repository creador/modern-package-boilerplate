    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "fs-file-tree": "^1.0.4",
    "mocha": "^3.3.0",
    "mocha-webpack": "^0.7.0",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-merge": "^4.1.0",
    "webpack-uglify-js-plugin": "^1.1.9"
